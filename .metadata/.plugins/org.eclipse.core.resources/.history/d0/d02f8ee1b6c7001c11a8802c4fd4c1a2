package date;

import java.util.Date;

public class test01 {
	/*
	 * 1. 2023년도 12.25일부터 오늘까지 몇 일인지
	 * 2. 오늘 날짜 표시
	 * 
	 */
	public static void main(String[] args) {
		//1. 2023년도 12.25일부터 오늘까지 몇 일인지
		Date date = new Date(0,0,0);
		System.out.println(date);
		date = new Date(10,1,1);
		System.out.println(date);
	}

}
